@model IEnumerable<RoRepairAdmin.Models.faqsmodel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbl.faqquestion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbl.faqanswer)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.City)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.tbl.faqquestion)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.tbl.faqanswer)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.tbl.faqid }) |
        @Html.ActionLink("Details", "Details", new { id = item.tbl.faqid }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.tbl.faqid })
    </td>
</tr>
}

</table>
